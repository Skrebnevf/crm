- entities = controller.controller_name

- if @signed_jobs.any?
  $('##{entities}').html('#{ j render(@signed_jobs) }');
- else
  $('##{entities}').html('#{ j render(partial: "shared/empty") }');

$('#paginate').html('#{ j render(partial: "shared/paginate_with_per_page") }');
$('#export').html('#{ j render(partial: "shared/export") }');
$('#search_results_count').html('#{ j render(plain: t('search_results_count', count: @search_results_count || @signed_jobs.size)) }');
